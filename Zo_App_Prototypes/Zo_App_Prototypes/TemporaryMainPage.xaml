<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Zo_App_Prototypes.TemporaryMainPage"
             Title="TemporaryMainPage"
             xmlns:vm="clr-namespace:Zo_App_Prototypes"
             x:DataType="vm:TemporaryMainPageVM">
    <VerticalStackLayout>
        <Label 
            Text="TEMPORARY MAIN PAGE"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Label 
	        Text="App MainPage"
	        VerticalOptions="Center" 
	        HorizontalOptions="Center" />

		<Button 
	        Text="Join"
           Command="{Binding NavigateToJoinPageCommand}"
	        VerticalOptions="Center" 
	        HorizontalOptions="Center" />


        <Button 
	        Text="Dashboard One"
	        Command="{Binding NavigateToDashboardOnePageCommand}"
	        VerticalOptions="Center" 
	        HorizontalOptions="Center" />

	</VerticalStackLayout>
</ContentPage>